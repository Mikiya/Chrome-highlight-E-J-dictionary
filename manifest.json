{
  "name": "Highlight J-E Dictionary",
  "version": "1.0.8",
  "description": "Search dictionary by selecting (highlighting) the text.",
  "content_scripts": [
      {
          "matches": [ "http://*/*", "https://*/*", "file://*/*" ],
          "js": ["js/json2.js", "js/jquery.js", "js/contents.js"],
          "css": ["css/default.css"]
      }
  ],
  "permissions": [ "http://*/*", "https://*/*", "tabs", "unlimitedStorage", "contextMenus" ],
  "background_page": "background.html",
  "options_page": "options.html",
  "default_locale": "en"
}
